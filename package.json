    "url": "git+https://github.com/coderextreme/x3dvalidate.git"
  },
  "keywords": [
    "JSON",
    ".x3dj",
    "Schema",
    "Validator",
    "X3D"
  ],
  "dependencies": {
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "console": "^0.7.2",
    "electron": "^25.1.1"
  },
  "bin": {
    "x3dvalidate": "x3dvalidate.js"
  },
  "scripts": {
    "postinstall": "node src/index.js",
    "publish": "npm publish --access public",
    "localtest": "node test/app.js",
    "test": "npx x3dvalidate examples/*.json"
  },
  "bugs": {
    "url": "https://github.com/coderextreme/x3dvalidate/issues"
